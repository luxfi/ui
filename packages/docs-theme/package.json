{
  "name": "@hanzo/docs-theme",
  "version": "1.0.0",
  "description": "Shared Fumadocs theme for Lux, Hanzo, and Zoo ecosystems",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./css/base": "./src/css/base.css",
    "./css/brands/lux": "./src/css/brands/lux.css",
    "./css/brands/hanzo": "./src/css/brands/hanzo.css",
    "./css/brands/zoo": "./src/css/brands/zoo.css",
    "./brands": {
      "import": "./src/brands/index.ts",
      "types": "./src/brands/index.ts"
    },
    "./components": {
      "import": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    },
    "./tailwind-preset": {
      "import": "./src/config/tailwind-preset.ts",
      "types": "./src/config/tailwind-preset.ts"
    }
  },
  "files": [
    "src"
  ],
  "keywords": [
    "fumadocs",
    "theme",
    "lux",
    "hanzo",
    "zoo",
    "documentation",
    "tailwindcss"
  ],
  "author": "Hanzo AI",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.4.0 || >=4.0.0",
    "fumadocs-ui": ">=14.0.0",
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.0",
    "lucide-react": "^0.460.0"
  }
}
